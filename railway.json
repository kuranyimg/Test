{
  "build": {
    "builder": "RailpackDefault",
    "buildCommand": "pip install -r requirements.txt",
    "watchPaths": ["**/*.py", "requirements.txt"]
  },
  "deploy": {
    "startCommand": "python run.py",
    "regions": ["southeastasia"],
    "restartPolicy": "on-failure",
    "maxRetries": 10,
    "cpu": 1,
    "memory": 512,
    "serverless": false
  },
  "networking": {
    "public": true,
    "healthcheckPath": "/"
  }
}
